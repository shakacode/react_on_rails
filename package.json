{
  "name": "react-on-rails-pro-vm-renderer",
  "version": "1.0.0",
  "protocolVersion": "1.0.0",
  "description": "react-on-rails-pro JavaScript for react_on_rails_pro Ruby gem",
  "main": "packages/vm-renderer/lib/ReactOnRailsProVmRenderer.js",
  "bin": {
    "vm-renderer": "./packages/vm-renderer/lib/default-vm-renderer.js"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "babel-tape-runner": "^2.0.1",
    "babel-types": "^6.25.0",
    "babelify": "^7.3.0",
    "console.history": "^1.5.0",
    "express": "^4.15.2",
    "express-busboy": "^6.0.1",
    "fs-extra": "^3.0.1",
    "semver": "^5.5.0",
    "supertest": "^3.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "blue-tape": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-shakacode": "^14.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.43.1",
    "jsdom": "^9.12.0",
    "release-it": "^2.7.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "files": [
    "packages/vm-renderer/lib"
  ],
  "scripts": {
    "test": "babel-tape-runner packages/vm-renderer/tests/*.js | tap-spec",
    "clean": "rm -rf packages/vm-renderer/lib",
    "babel": "babel --out-dir packages/vm-renderer/lib packages/vm-renderer/src",
    "build": "yarn run clean && yarn run babel",
    "build:dev": "yarn run clean && yarn run babel -w",
    "postinstall": "yarn run build",
    "build-watch": "babel --watch --out-dir packages/vm-renderer/lib node_package/src",
    "prepublish": "yarn run build",
    "prerelease": "yarn run check && yarn run clean && yarn run build",
    "start": "echo 'Starting React on Rails Pro VM Renderer using the default configuration.' && node packages/vm-renderer/lib/default-vm-renderer.js",
    "eslint": "eslint .",
    "flow": "flow check packages/vm-renderer",
    "lint": "yarn run eslint",
    "check": "yarn run lint && yarn run flow && yarn run test",
    "local": "echo 'Be sure to run yarn run dummy:install' && yarn run build && cd spec/dummy && foreman start",
    "link": "yarn link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakacode/react_on_rails_pro.git"
  },
  "keywords": [
    "react",
    "webpack",
    "JavaScript",
    "Ruby",
    "on",
    "Rails"
  ],
  "author": "justin@shakacode.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shakacode/react_on_rails_pro/issues"
  },
  "homepage": "https://github.com/shakacode/react_on_rails_pro#readme"
}
