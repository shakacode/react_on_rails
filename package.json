{
  "name": "react-on-rails-pro-vm-renderer",
  "version": "1.2.1",
  "protocolVersion": "1.0.0",
  "description": "react-on-rails-pro JavaScript for react_on_rails_pro Ruby gem",
  "main": "packages/vm-renderer/src/ReactOnRailsProVmRenderer.js",
  "bin": {
    "vm-renderer": "./packages/vm-renderer/src/default-vm-renderer.js"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "concurrently": "^4.0.1",
    "console.history": "^1.5.0",
    "eslint-config-prettier": "^3.1.0",
    "express": "^4.15.2",
    "express-busboy": "^7.0.1",
    "fs-extra": "^8.0.1",
    "honeybadger": "^1.3.0",
    "lockfile": "^1.0.4",
    "semver": "^5.5.0",
    "sleep-promise": "^8.0.1",
    "source-map-support": "^0.5.9",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.1",
    "eslint-config-shakacode": "^17.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "jsdom": "^13.1.0",
    "nps": "^5.9.3",
    "prettier": "^1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "release-it": "^10.0.1",
    "supertest": "^3.0.0",
    "tap-diff": "^0.1.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.6.3",
    "tape-async": "^2.3.0",
    "touch": "^3.1.0",
    "yalc": "^1.0.0-pre.27"
  },
  "files": ["packages/vm-renderer/src"],
  "scripts": {
    "test": "jest packages/vm-renderer/tests/**/*.test.js",
    "prerelease": "yarn run check",
    "start": "nps",
    "developing": "rm -rf /tmp/react-on-rails-pro-vm-renderer-bundles && RENDERER_LOG_LEVEL=info NODE_DEBUG=ROR node  -r source-map-support/register packages/vm-renderer/src/default-vm-renderer.js",
    "eslint": "eslint .",
    "lint": "yarn run eslint",
    "check": "yarn run lint && yarn run test",
    "local": "echo 'Be sure to run yarn run dummy:install' && yarn run build && cd spec/dummy && foreman start",
    "link": "yarn link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakacode/react_on_rails_pro.git"
  },
  "keywords": ["react", "webpack", "JavaScript", "Ruby", "on", "Rails"],
  "author": "justin@shakacode.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shakacode/react_on_rails_pro/issues"
  },
  "homepage": "https://github.com/shakacode/react_on_rails_pro#readme",
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": ["packages/vm-renderer/tests/**/*.{js,jsx}"],
    "coverageReporters": ["lcov"],
    "resetModules": true,
    "resetMocks": true,
    "setupFiles": ["./packages/vm-renderer/tests/helper.js"],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yalc check"
    }
  }
}
