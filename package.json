{
  "name": "react-on-rails-workspace",
  "version": "16.1.1",
  "description": "react-on-rails monorepo workspace manager",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/react-on-rails"
  ],
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@babel/core": "^7.20.12",
