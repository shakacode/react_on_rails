{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["dom", "es2015"],
    "module": "es2015",
    "moduleResolution": "bundler",
    "noImplicitAny": true,
    "outDir": "node_package/lib",
    "strict": true,
    "incremental": true,
    "target": "es2015"
  },
  "include": ["node_package/src/**/*"]
}
