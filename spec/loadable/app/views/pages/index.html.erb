<% react_app = react_component_hash("App", props: @props, id: "loadable-react-component-1") %>
<% content_for :title do %>
  <%= react_app['title'] if react_app['title']&.include?('<') %>
<% end %>
<% content_for :head do %>
  <%= react_app['meta'].html_safe if react_app['meta']&.include?('<') %>
  <%= react_app['link'].html_safe if react_app['link']&.include?('<') %>
  <%= react_app['style'].html_safe if react_app['style']&.include?('<') %>
<% end %>
<%= react_app['linkTags'] %>
<%= react_app['componentHtml'] %>
<%= react_app['scriptTags'] %>
