    "build:test": "rm -rf public/webpack/test && RAILS_ENV=test NODE_ENV=test bin/webpacker",
    "build:dev": "rm -rf public/webpack/development && RAILS_ENV=development NODE_ENV=development bin/webpacker",
    "build:dev:watch": "RAILS_ENV=development NODE_ENV=development bin/webpacker --watch",
    "build:client": "RAILS_ENV=production NODE_ENV=production bin/webpacker",
    "build:server": "RAILS_ENV=production NODE_ENV=production SERVER=true bin/webpacker",
    "build:clean": "rm -rf public/webpack || true",
    "node-renderer-debug": "RENDERER_PORT=3800 ndb client/node-renderer.js",
    "node-renderer": "RENDERER_PORT=3800 node client/node-renderer.js"
