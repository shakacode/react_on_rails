    "build:dev:client": "(cd client && yarn run build:dev:client)",
    "build:dev:server": "(cd client && yarn run build:dev:server)",
    "hot-assets": "(cd client && yarn run hot-assets)"
  },
  "repository": {
    "type": "git",
