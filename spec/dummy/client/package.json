    "es5-shim": "^4.5.7",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "^2.0.1",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.2",
    "jquery-ujs": "^1.2.1",
    "loader-utils": "^0.2.13",
    "lodash": "^4.7.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.2",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-on-rails": "../../..",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
